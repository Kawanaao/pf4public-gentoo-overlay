  "dependencies": {
    "arg": "^5.0.2",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "flat-cache": "^6.1.0",
    "glob": "^11.0.0",
    "minimatch": "^5.1.0",
    "morgan": "^1.10.0",
    "ssri": "^10.0.0",
    "tar": "^6.1.11",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/glob": "^8.1.0",
